{
    "name": "dendreo/urlcrypt",
    "description": "PHP library to securely encode and decode short pieces of arbitrary binary data in URLs.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dendreo",
            "email": "devs@dendreo.com",
            "homepage": "https://dendreo.com",
            "role": "dev"
        }
    ],
    "support": {
        "email": "devs@dendreo.com"
    },
    "require": {
        "php": "^8.0",
        "ext-openssl": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {"Atrapalo\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Atrapalo\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
